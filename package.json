{
  "name": "getting-started-iv-angular",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "format": "prettier --ignore-path .gitignore --ignore-pattern node_modules --write src",
    "install:ci": "npm install && (npm list || true)",
    "build:ci": "npm run install:ci && npm run build && pwd && ls -lrt",
    "test:ci": "ng test --karma-config karma.pipeline.conf.js --code-coverage --watch false",
    "cleanup": "rm -rf package-lock.json && rm -rf node_modules && rm -rf www",
    "addAndroid": "rm -rf android && npx cap add android",
    "addIos": "rm -rf ios && npx cap add ios",
    "capSync": "npx cap sync",
    "openAndroid": "npm run build && npm run addAndroid && npm run capSync android && npx cap open android",
    "openIos": "npm run build && npm run addIos && npm run capSync ios && npx cap open ios",
    "mobileBuilds": "npm run openAndroid && npm run openIos",
    "livereload:android": "npx ionic cap run android -l --port=8200",
    "livereload:ios": "npx ionic cap run ios -l --port=8100",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~11.2.0",
    "@angular/core": "~11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "@angular/router": "~11.2.0",
    "@awesome-cordova-plugins/app-rate": "^5.37.3",
    "@awesome-cordova-plugins/core": "^5.37.3",

    "@capacitor/android": "^4.0.0",
    "@capacitor/app": "^4.0.0",
    "@capacitor/core": "^4.0.0",
    "@capacitor/device": "^4.0.0",
    "@capacitor/filesystem": "^4.0.0",
    "@capacitor/ios": "^4.0.0",
    "@capacitor/keyboard": "^4.0.0",
    "@capacitor/status-bar": "^4.0.0",
    "@ionic-enterprise/identity-vault": "5.7.1",
    "@ionic-native/core": "^5.32.1",
    "@ionic-native/http": "^5.33.0",
    "@ionic-native/in-app-browser": "^5.35.0",
    "@ionic-native/preview-any-file": "^5.36.0",
    "@ionic/angular": "^5.5.2",
    "cordova-plugin-advanced-http": "^3.1.0",
    "cordova-plugin-androidx": "^3.0.0",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "cordova-plugin-apprate": "^1.7.2",
    "cordova-plugin-dialogs": "^2.0.2",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-nativestorage": "^2.3.2",
    "cordova-plugin-preview-any-file": "^0.2.9",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.4",
    "@angular-eslint/builder": "2.0.2",
    "@angular/cli": "~11.2.4",
    "@angular/compiler": "~11.2.0",
    "@angular/compiler-cli": "~11.2.0",
    "@capacitor/cli": "^4.0.0",
    "@ionic/angular-toolkit": "^3.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.15",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-sauce-launcher": "^4.3.6",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2"
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html}": [
      "ng lint --fix",
      "npm run format"
    ]
  },
  "description": "An Ionic project"
}
